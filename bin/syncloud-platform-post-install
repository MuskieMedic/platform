#!/bin/bash

echo "update packages"
apt-get -y update
echo "fix apt errors"
apt-get -yf install

echo "installing insider dependencies"
apt-get -y install miniupnpc
echo "running syncloud base image tweaks"
syncloud-base-installer
echo  "installing java"
install-java

syncloud-boot-installer
syncloud-remote-post-install
syncloud-apache-post-install
syncloud-server-post-install
install-avahi